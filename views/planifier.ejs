<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/header.ejs') %>
</head>
<body>
    <div class="container">
        <%- include('./partials/navbare.ejs') %>   
        <section class="menu">
            <div class="box">
                    <div class="insidebox semestre">
                        <p>Choisit le semestre :</p>
                        <select name="semestre" id="semestre">
                            <option value="1">Semestre 1</option>
                            <option value="2">Semestre 2</option>
                        </select>
                    </div>
                    <div class="insidebox palier">
                        <p>Choisit le palier :</p>
                        <div class="form">
                            <select name="palier" id="palier">
                                <option value="">License</option>
                                <option value="">Master</option>
                            </select>
                        </div>
                    </div>
                    <div class="insidebox specialites">
                        <p>Choisit la spécialité :</p>
                        <div class="form">
                            <select name="specialite" id="specialite" class="sp">
                                <% 
                                 for(let index in licence){%>
                                    <option value="<%= licence[index].code_specialite %>"> <%= licence[index].code_specialite %> </option>
                                 <%}%>
                            </select>
                        </div>
                    </div>
                    <div class="insidebox niveau">
                        <p>Choisit le niveau :</p>
                        <select name="niveau" id="niveau">
                            <option value="">L2</option>
                            <option value="">L3</option>
                        </select>
                    </div>
                    <div class="insidebox sections">
                        <p>Choisit la section :</p>
                        <select name="section" id="section">
                            <option value="">A</option>
                            <option value="">B</option>
                            <option value="">C</option>
                        </select>
                    </div>
                    <div class="button">
                        <button>Confirmer</button>
                    </div>
            </div>
        </section>
    </div>
    <script>

       
    </script>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200&display=swap');
    </style>
</body>
</html>